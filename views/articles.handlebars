<div class="row">
    <div class="col-1"></div>
    <div class="col-10 articles">
        <h1 class="savedArt">Saved Articles</h1>
        <ul>
            {{#each savedArticles}}
                <p style="margin-top:1%;">{{this.title}}</p><br>
                <a href="{{this.link}}" target="_blank">{{this.link}}</a> 
                <hr> 
            {{/each}}
        </ul>
    </div>
    <div class="col-1"></div>
</div>
<div class="row">
    <div class="col-1"></div>
    <div class="col-2">
        <a href="/" class="artButton">
            <button type="button" class="btn btn-light">Back to All Articles</button>
        </a>
    </div>
    <div class="col-9"></div>
</div>





